{
	"name": "JobHubWebservice",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"prestart": "npm run -s build",
		"start": "node dist/app",
		"dev": "nodemon src/app --exec \"npm run lint && node -r babel-register\"",
		"clean": "rimraf dist",
		"build": "npm run clean && mkdir dist && babel src -s -d dist",
		"test": "mocha --opts ./mocha.opts __test__/**/**/*.spec.js",
		"test:current": "mocha --opts ./mocha.opts __test__/**/unit/*.spec.js",
		"lint": "esw src"
	},
	"dependencies": {
		"assert": "^1.4.1",
		"babel-cli": "^6.26.0",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"body-parser": "~1.17.1",
		"config": "^1.28.1",
		"cookie-parser": "~1.4.3",
		"cors": "^2.8.4",
		"debug": "~2.6.3",
		"dotenv": "^4.0.0",
		"express": "~4.15.2",
		"mongoose": "^5.0.0-rc1",
		"morgan": "~1.8.1",
		"pluralize": "^7.0.0",
		"q": "^1.5.1",
		"query-string": "^5.0.1",
		"serve-favicon": "^2.4.5",
		"underscore": "^1.8.3",
		"validatorjs": "^3.13.5"
	},
	"devDependencies": {
		"babel-eslint": "^8.0.2",
		"babel-register": "^6.26.0",
		"chai": "^4.1.2",
		"env-test": "^1.0.0",
		"eslint": "^4.11.0",
		"eslint-config-google": "^0.9.1",
		"eslint-plugin-import": "^2.8.0",
		"eslint-plugin-mocha": "^4.11.0",
		"eslint-watch": "^3.1.3",
		"mocha": "^4.0.1",
		"npm-run-all": "^4.1.2",
		"should": "^13.1.3",
		"sinon": "^4.1.2",
		"supertest": "^3.0.0"
	}
}
